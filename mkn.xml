<?xml version="1.0" encoding="UTF-8" ?>
<maiken>
	<project>
		<name>fragex</name>
		<version>master</version>
	</project>
	<dependencies>
		<dependency>
			<name>kul.cgi.irr.2D</name>
			<version>master</version>
			<!--remote scm="git">http://www.github.com/Dekken/kul.cgi.git</remote-->
		</dependency>
	</dependencies>
	<includes>
		<include>./inc</include>
	</includes>
	<build type="exe" mode="static" main="./main.cpp">
		<files>
			<file type="cpp">
				<args>
					<arg>-std=c++0x</arg><arg>-D__GXX_EXPERIMENTAL_CXX0X__</arg>
					<arg>-Wall</arg><arg>-fmessage-length=0</arg>
					<arg>-O0</arg><arg>-g3</arg>
				</args>
			</file>
		</files>
	</build>
</maiken>
